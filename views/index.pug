extends layout

block content
  h1 파일 확장자 차단
  table 
    tr
      th 고정 확장자
      td 
        -for (let i=0; i<extension.length; i++)
          input(type="checkbox", name="fixed", id="fixed_#{i}")
          label(for="fixed_#{i}") #{extension[i]}
    tr 
      th 커스텀 확장자
      td 
        input(type="text", maxlength="10")
        buttion(type="button", onclick="add()") 추가

